{
  "name": "dashboard",
  "description": "Customizable personal dashboard and startpage",
  "version": "1.5.0",
  "author": "Darek Kay <hello@darekkay.com> (https://darekkay.com/)",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@darekkay/styles": "2.4.0",
    "@fortawesome/fontawesome-svg-core": "1.2.32",
    "@fortawesome/free-brands-svg-icons": "5.15.1",
    "@fortawesome/free-regular-svg-icons": "5.15.1",
    "@fortawesome/free-solid-svg-icons": "5.15.1",
    "@fortawesome/react-fontawesome": "0.1.13",
    "@reduxjs/toolkit": "1.4.0",
    "axios": "0.21.0",
    "classnames": "2.2.6",
    "dayjs": "1.9.6",
    "eslint": "7.25.0",
    "i18next": "19.8.4",
    "lodash": "4.17.20",
    "loglevel": "1.7.1",
    "qrcode.react": "1.0.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-dropzone": "11.2.4",
    "react-full-screen": "0.2.4",
    "react-grid-layout": "1.2.2",
    "react-i18next": "11.7.3",
    "react-measure": "2.5.2",
    "react-modal": "3.12.1",
    "react-redux": "7.2.2",
    "react-scripts": "4.0.2",
    "react-sizeme": "3.0.1",
    "reakit": "1.3.1",
    "recharts": "2.0.9",
    "redux-persist": "6.0.0",
    "redux-sagas-injector": "1.1.2",
    "selectorator": "4.0.3",
    "typed-redux-saga": "1.3.1"
  },
  "devDependencies": {
    "@darekkay/logger": "1.1.2",
    "@darekkay/scripts": "1.2.0",
    "@storybook/addons": "6.2.9",
    "@storybook/preset-create-react-app": "3.1.7",
    "@storybook/react": "6.2.9",
    "@tailwindcss/postcss7-compat": "2.0.1",
    "@testing-library/jest-dom": "5.11.6",
    "@testing-library/react": "11.2.2",
    "@testing-library/react-hooks": "3.4.2",
    "@testing-library/user-event": "12.2.2",
    "@types/classnames": "2.2.11",
    "@types/jest": "26.0.15",
    "@types/lodash": "4.14.165",
    "@types/node": "14.14.10",
    "@types/qrcode.react": "1.0.1",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/react-grid-layout": "1.1.1",
    "@types/react-measure": "2.0.6",
    "@types/react-modal": "3.10.6",
    "@types/react-redux": "7.1.11",
    "babel-eslint": "10.1.0",
    "plop": "2.7.4",
    "react-test-renderer": "17.0.1",
    "redux-devtools-extension": "2.13.8",
    "sass": "1.32.13",
    "source-map-explorer": "2.5.1",
    "storybook-addon-themes": "6.1.0",
    "swagger-typescript-api": "9.1.0",
    "ts-node": "9.1.1",
    "typescript": "4.2.4"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "homepage": "https://dashboard.darekkay.com",
  "jest": {
    "coverageThreshold": {
      "global": {
        "statements": 87,
        "branches": 80,
        "functions": 82,
        "lines": 87
      }
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!**/node_modules/**",
      "!**/__stories__/**",
      "!**/react-app-env.d.ts",
      "!src/api/routes*",
      "!src/components/**/selectors.ts",
      "!src/common/utils/mock.tsx",
      "!src/widgets/list.ts",
      "!src/widgets/**/properties.ts",
      "!src/widgets/**/configuration.tsx",
      "!src/widgets/**/sagas.ts"
    ]
  },
  "license": "MIT",
  "main": "src/index.tsx",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/darekkay/dashboard.git"
  },
  "resolutions": {
    "@typescript-eslint/parser": "4.23.0",
    "babel-loader": "8.1.0",
    "eslint": "7.25.0",
    "eslint-plugin-react": "7.23.2",
    "eslint-plugin-testing-library": "4.3.0"
  },
  "scripts": {
    "analyze": "source-map-explorer \"build/static/js/*.js\"",
    "build": "react-scripts build",
    "ci": "dks ci",
    "format": "dks format",
    "generate": "plop --plopfile internals/generators/index.js",
    "i18n": "ts-node -P internals/scripts/tsconfig.json internals/scripts/i18n.ts ",
    "lint": "dks lint",
    "scan": "run-p scan:*",
    "scan:api": "swagger-typescript-api --axios --route-types --js -p ../server/src/api/swagger.json -o src/api -n routes.ts",
    "scan:widgets": "ts-node -P internals/scripts/tsconfig.json internals/scripts/scan-widgets.ts && prettier --write src/widgets/list.ts && prettier --write ../server/src/widgets.ts",
    "start": "react-scripts start",
    "storybook:build": "build-storybook",
    "storybook:start": "start-storybook -p 6006 -s .storybook/assets",
    "tailwind": "tailwind build internals/tailwind/tailwind.css -c internals/tailwind/config.js --no-autoprefixer -o src/styles/_tailwind.scss && prettier --write src/styles/_tailwind.scss",
    "test": "react-scripts test --modulePaths=src --coverage",
    "typecheck": "dks typecheck"
  }
}
