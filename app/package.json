  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/darekkay/dashboard.git"
  },
  "scripts": {
    "build": "vite build",
    "ci": "dks ci",
    "clean-demo": "ts-node -P internals/scripts/tsconfig.json internals/scripts/clean-demo.ts",
    "format": "dks format",
    "generate": "plop --plopfile internals/generators/index.js",
    "i18n": "ts-node -P internals/scripts/tsconfig.json internals/scripts/i18n.ts ",
    "lint": "dks lint",
    "scan": "run-p scan:*",
    "scan:api": "swagger-typescript-api --axios --route-types --js -p ../server/src/api/swagger.json -o src/api -n routes.ts",
    "scan:widgets": "ts-node --transpile-only -P internals/scripts/tsconfig.json internals/scripts/scan-widgets.ts && prettier --write src/widgets/list.ts && prettier --write ../server/src/widgets.ts",
    "start": "vite",
    "tailwind": "tailwind build -i internals/tailwind/tailwind.css -c internals/tailwind/config.js --no-autoprefixer -o src/styles/_tailwind.scss && prettier --write src/styles/_tailwind.scss",
    "test": "dks test --coverage",
    "typecheck": "dks typecheck"
  }
}
