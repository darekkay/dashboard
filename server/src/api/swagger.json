	"paths": {
		"/cryptocurrencies/price": {
			"get": {
				"operationId": "GetCryptocurrencyPrice",
				"responses": {
					"200": {
						"description": "Ok",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/CryptocurrencyPrice"
								}
							}
						}
					}
				},
				"description": "Returns the current price for a cryptocurrency.",
				"security": [],
				"parameters": [
					{
						"description": "cryptocurrency code, e.g. \"bitcoin\"",
						"in": "query",
						"name": "crypto",
						"required": true,
						"schema": {
							"type": "string"
						}
					},
					{
						"description": "fiat currency code, e.g. \"eur\"",
						"in": "query",
						"name": "fiat",
						"required": true,
						"schema": {
							"type": "string"
						}
					}
				]
			}
		},
		"/github/stats": {
			"get": {
				"operationId": "GetGitHubStats",
				"responses": {
					"200": {
						"description": "Ok",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/GitHubStats"
								}
							}
						}
					},
					"422": {
						"description": "Unprocessable entity",
						"content": {
							"application/json": {
								"schema": {
									"properties": {
										"reason": {
											"type": "string"
										}
									},
									"required": [
										"reason"
									],
									"type": "object"
								}
							}
						}
					}
				},
				"description": "Returns stats for a GitHub user or repository.",
				"security": [],
				"parameters": [
					{
						"description": "GitHub user or repository",
						"in": "query",
						"name": "query",
						"required": true,
						"schema": {
							"type": "string"
						}
					}
				]
			}
		},
		"/unsplash/random": {
			"get": {
				"operationId": "GetRandomImage",
				"responses": {
					"200": {
						"description": "Ok",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/UnsplashImage"
								}
							}
						}
					}
				},
				"description": "Returns a random Unsplash image.",
				"security": [],
				"parameters": []
			}
		},
		"/weather": {
			"get": {
				"operationId": "GetWeatherData",
				"responses": {
					"200": {
						"description": "Ok",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/WeatherData"
								}
							}
						}
					}
				},
				"description": "Returns weather data.",
				"security": [],
				"parameters": [
					{
						"description": "Latitude",
						"in": "query",
						"name": "lat",
						"required": true,
						"schema": {
							"type": "string"
						}
					},
					{
						"description": "Longtitude",
						"in": "query",
						"name": "lon",
						"required": true,
						"schema": {
							"type": "string"
						}
					},
					{
						"description": "Unit",
						"in": "query",
						"name": "unit",
						"required": true,
						"schema": {
							"type": "string",
							"enum": [
								"imperial",
								"metric"
							]
						}
					}
				]
			}
		},
		"/youtube/stats": {
			"get": {
				"operationId": "GetYoutubeStats",
				"responses": {
					"200": {
						"description": "Ok",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/YoutubeStats"
								}
							}
						}
					},
					"404": {
						"description": "Not found",
						"content": {
							"application/json": {
								"schema": {
									"properties": {},
									"type": "object"
								}
							}
						}
					}
				},
				"description": "Returns stats for a YouTube account.",
				"security": [],
				"parameters": [
					{
						"description": "Youtube id or username",
						"in": "query",
						"name": "query",
						"required": true,
						"schema": {
							"type": "string"
						}
					}
				]
			}
		}
