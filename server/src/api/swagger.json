{
	"components": {
		"examples": {},
		"headers": {},
		"parameters": {},
		"requestBodies": {},
		"responses": {},
		"schemas": {
			"CryptocurrencyPrice": {
				"properties": {
					"currentPrice": {
						"type": "number",
						"format": "double"
					},
					"last24h": {
						"properties": {
							"changePercentage": {
								"type": "number",
								"format": "double"
							},
							"change": {
								"type": "number",
								"format": "double"
							}
						},
						"required": [
							"changePercentage",
							"change"
						],
						"type": "object"
					},
					"imageUrl": {
						"type": "string"
					}
				},
				"required": [
					"currentPrice",
					"last24h"
				],
				"type": "object",
				"additionalProperties": true
			},
			"GitHubStats": {
				"properties": {
					"name": {
						"type": "string"
					},
					"followers": {
						"type": "number",
						"format": "double"
					},
					"stars": {
						"type": "number",
						"format": "double"
					},
					"subscribers": {
						"type": "number",
						"format": "double"
					},
					"forks": {
						"type": "number",
						"format": "double"
					},
					"open_issues": {
						"type": "number",
						"format": "double"
					}
				},
				"type": "object",
				"additionalProperties": true
			},
			"UnsplashImage": {
				"properties": {
					"imageUrl": {
						"type": "string"
					},
					"authorName": {
						"type": "string"
					},
					"authorUrl": {
						"type": "string"
					},
					"altText": {
						"type": "string"
					}
				},
				"type": "object",
				"additionalProperties": true
			},
			"WeatherCondition": {
				"properties": {
					"description": {
						"type": "string"
					},
					"icon": {
						"type": "string",
						"enum": [
							"01d",
							"02d",
							"03d",
							"04d",
							"09d",
							"10d",
							"11d",
							"13d",
							"50d",
							"01n",
							"02n",
							"03n",
							"04n",
							"09n",
							"10n",
							"11n",
							"13n",
							"50n"
						]
					}
				},
				"required": [
					"description",
					"icon"
				],
				"type": "object",
				"additionalProperties": true
			},
			"WeatherData": {
				"properties": {
					"current": {
						"properties": {
							"condition": {
								"$ref": "#/components/schemas/WeatherCondition"
							},
							"temperature": {
								"type": "number",
								"format": "double"
							}
						},
						"required": [
							"condition",
							"temperature"
						],
						"type": "object"
					},
					"forecast": {
						"items": {
							"properties": {
								"condition": {
									"$ref": "#/components/schemas/WeatherCondition"
								},
								"temperatureMax": {
									"type": "number",
									"format": "double"
								},
								"temperatureMin": {
									"type": "number",
									"format": "double"
								},
								"date": {
									"type": "number",
									"format": "double"
								}
							},
							"required": [
								"condition",
								"temperatureMax",
								"temperatureMin",
								"date"
							],
							"type": "object"
						},
						"type": "array"
					}
				},
				"required": [
					"current",
					"forecast"
				],
				"type": "object",
				"additionalProperties": true
			},
			"YoutubeStats": {
				"properties": {
					"channelTitle": {
						"type": "string"
					},
					"subscriberCount": {
						"type": "number",
						"format": "double"
					},
					"viewCount": {
						"type": "number",
						"format": "double"
					},
					"videoCount": {
						"type": "number",
						"format": "double"
					}
				},
				"type": "object",
				"additionalProperties": true
			}
		},
		"securitySchemes": {}
	},
	"info": {
		"title": "dashboard-server",
		"version": "1.5.0",
		"description": "A server for the dashboard app",
