    "cors": "2.8.5",
    "dayjs": "1.10.7",
    "dotenv": "10.0.0",
    "express": "4.17.2",
    "lodash": "4.17.21",
    "loglevel": "1.8.0",
    "morgan": "1.10.0",
    "swagger-ui-express": "4.3.0",
    "tsoa": "3.14.1"
  },
  "devDependencies": {
    "@darekkay/scripts": "2.4.0",
    "@types/body-parser": "1.19.2",
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/express-serve-static-core": "4.17.27",
    "@types/jest": "27.0.3",
    "@types/lodash": "4.14.178",
    "@types/morgan": "1.9.3",
    "@types/node": "16.11.17",
    "@types/serve-static": "1.13.10",
    "@types/supertest": "2.0.11",
    "@types/swagger-ui-express": "4.1.3",
    "jest": "28.1.0",
    "nodemon": "2.0.15",
    "plop": "3.0.5",
    "supertest": "6.1.6",
    "ts-jest": "28.0.2",
    "ts-node": "10.4.0",
    "typescript": "4.5.4"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "homepage": "https://dashboard.darekkay.com",
  "license": "MIT",
  "main": "index.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/darekkay/dashboard-server.git"
  },
  "scripts": {
    "build": "tsc",
    "build-api": "tsoa spec-and-routes",
    "build-api:dev": "nodemon -x tsoa spec-and-routes --ignore src/api",
    "ci": "dks ci",
    "dev": "nodemon src/index.ts",
    "format": "dks format",
    "generate": "plop --plopfile internals/generators/index.js",
    "lint": "dks lint",
    "start": "ts-node src/index.ts",
    "test": "dks test --coverage",
    "typecheck": "dks typecheck"
  }
}
